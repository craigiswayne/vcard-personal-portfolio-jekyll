<section class="clients">
    <h3 class="h3 clients-title">Clients</h3>
    <ul class="clients-list has-scrollbar">
        {% for client in site.data.clients %}
            <li class="clients-item">
                <div href="#">
                    <img src="{{ client.imageURL }}" alt="{{ client.name }} logo" />
                </div>
            </li>
        {% endfor %}
    </ul>
</section>